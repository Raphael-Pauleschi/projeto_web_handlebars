<div class="container text-center">
  <div class="row">
    <div class="col-1">
      &nbsp;
    </div>
    <div class="col bg-primary">
      <nav class="navbar bg-primary navbar-expand-lg" data-bs-theme="dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="#">SISTEMA WEB</a>
          <button
            class="navbar-toggler"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarNavAltMarkup"
            aria-controls="navbarNavAltMarkup"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
              <a
                class="nav-link active"
                aria-current="page"
                href="/"
              >Cadastrar</a>
              <a class="nav-link" href="/consulta">Consultar</a>
            </div>
          </div>
        </div>
      </nav>
    </div>
    <div class="col-1">
      &nbsp;
    </div>
  </div>
  <div class="row">
    <div class="col-1">
      &nbsp;
    </div>
    <div class="col bg-white">
      &nbsp;
    </div>
    <div class="col-1">
      &nbsp;
    </div>
  </div>
  <div class="row">
    <div class="col-1">
      &nbsp;
    </div>
    <div class="col bg-white">
      <p class="text-start fs-5">Consultar - Agendamento de Potenciais Clientes</p>
      <p class="text-start fs-6">Sistema Utilizado para agendamento de serviços.</p>
      <p>
        <div class="table-responsive container text-center">
        <table>
          <thead>
		  <tr class="bg-primary">
            <th scope="row" class="text-light">
              Nome 
            </th>
            <th scope="row" class="text-light">
              Telefone
            </th>
            <th scope="row" class="text-light">
              Origem
            </th>
            <th scope="row" class="text-light">
              Data de Contato
            </th>
           <th scope="row" class="text-light">
              Observação
            </th>
            <th scope="row" class="text-light">
              Ações
            </th>
          </tr>
		  </thead>
		  <tbody>
          {{#each post}}
            {{#dataValues}}
              <tr>
			  <td scope="row">
                  {{nome}}
                </td>
               <td scope="row">
                  {{telefone}}
                </td>
               <td scope="row">
                  {{origem}}
                </td>
                <td scope="row">
                  {{data_contato}}
                </td>
                <td scope="row">
                  {{observacao}}
                </td>
                <td scope="row">
                  <button type="button" class="btn btn-primary"><a
                      href="/atualizar/{{id}}"
                      style="color:inherit"
                    >Editar</a></button>
                  <button type="button" class="btn btn-primary"><a
                      href=""
                      style="color:inherit"
                    >Excluir</a></button>
                </td>
              </tr>
            {{/dataValues}}
          {{/each}}
        </tbody>
        </table>
        </div>
      </p>
    </div>
    <div class="col-1">
      &nbsp;
    </div>
  </div>
</div>